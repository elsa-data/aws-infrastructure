{
  "name": "@elsa-data/aws-infrastructure",
  "version": "0.0.0",
  "description": "A CDK stack that deploys well-configured infrastructure for use by Elsa Data",
  "author": {
    "name": "University of Melbourne Centre for Cancer Research",
    "organization": true
  },
  "main": "infrastructure-stack.js",
  "types": "infrastructure-stack.d.ts",
  "homepage": "https://github.com/elsa-data",
  "repository": {
    "type": "git",
    "url": "https://github.com/elsa-data/aws-infrastructure/stack"
  },
  "license": "MIT",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak -v",
    "clean": "git clean -fXd"
  },
  "keywords": [
    "elsa-data",
    "jsii",
    "cdk"
  ],
  "jsii": {
    "outdir": "dist",
    "targets": {},
    "tsc": {
      "paths": {
        "@common/*": [
          "../common/*"
        ]
      }
    }
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "aws-cdk": "2.100.0",
    "aws-cdk-lib": "2.100.0",
    "constructs": "10.3.0",
    "jsii": "5.2.14",
    "jsii-pacmak": "1.90.0",
    "publib": "0.2.737"
  }
}
