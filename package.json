{
  "name": "elsa-data-aws-deploy-infrastructure",
  "version": "1.0.0",
  "private": true,
  "description": "A manually deployable CDK stack for Elsa Data infrastructure in the UMCCR environment",
  "scripts": {
    "dev-deploy": "cdk --app 'npx ts-node infrastructure.ts' deploy ElsaDataDevInfrastructureStack",
    "dev-destroy": "cdk --app 'npx ts-node infrastructure.ts' destroy ElsaDataDevInfrastructureStack",
    "agdemo-deploy": "cdk --app 'npx ts-node infrastructure.ts' deploy ElsaDataDemoAustralianGenomicsInfrastructureStack",
    "agdemo-destroy": "cdk --app 'npx ts-node infrastructure.ts' destroy ElsaDataDemoAustralianGenomicsInfrastructureStack",
    "ag-deploy": "cdk --app 'npx ts-node infrastructure.ts' deploy ElsaDataProdAustralianGenomicsInfrastructureStack",
    "ag-destroy": "cdk --app 'npx ts-node infrastructure.ts' destroy ElsaDataProdAustralianGenomicsInfrastructureStack"
  },
  "devDependencies": {
    "@tsconfig/node18": "2.0.1",
    "@types/lodash": "4.14.195",
    "@types/node": "18.16.3",
    "aws-cdk": "2.86.0",
    "prettier": "2.8.8",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "aws-cdk-lib": "2.86.0",
    "constructs": "10.2.69",
    "source-map-support": "0.5.21",
    "lodash": "4.17.21"
  }
}
