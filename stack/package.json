    "elsa-data-aws-infrastructure-shared"
  ],
  "devDependencies": {
    "@types/lodash": "4.14.197",
    "aws-cdk": "2.80.0",
    "aws-cdk-lib": "2.80.0",
    "constructs": "10.2.0",
    "jsii": "5.1.10",
    "jsii-pacmak": "1.86.1",
    "prettier": "3.0.1",
    "publib": "0.2.737",
    "typescript": "5.1.6"
  }
