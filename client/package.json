{
  "name": "@umccr/elsa-data-aws-infrastructure-client",
  "version": "0.0.0",
  "description": "A library for clients wanting to use the Elsa Data infrastructure stack",
  "author": {
    "name": "University of Melbourne Centre for Cancer Research",
    "organization": true
  },
  "main": "infrastructure-client.ts",
  "types": "infrastructure-client.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/umccr/elsa-data-aws-infrastructure/client"
  },
  "license": "MIT",
