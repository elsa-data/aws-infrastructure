  },
  "bundledDependencies": [
    "elsa-data-aws-infrastructure-shared"
  ],
  "devDependencies": {
    "aws-cdk-lib": "2.50.0",
    "constructs": "10.2.0",
    "jsii": "5.1.10",
    "jsii-pacmak": "1.86.1",
    "prettier": "3.0.1",
    "publib": "0.2.737",
    "typescript": "5.1.6"
  }
